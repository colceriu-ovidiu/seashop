{% set curr_menu = 'users' %}
{% extends "COEShopBundle::admin_layout.html.twig" %}


{% block left_side %}
{% endblock %}

{% block content %}
<div class="top-bar">
	<a href="{{ path('admin_category_edit', { 'id': 0 }) }}" class="button">ADD NEW </a>
	<h1>Users list</h1>
	<div class="breadcrumbs"><a href="#">Homepage</a> / <a href="#">Contents</a></div>
</div><br />

{% if app.session.flash('notice') %}
		<div class="flash-message">
				<em>Notice</em>: {{ app.session.flash('notice') }}
		</div>
{% endif %}


<div class="table">
<table class="listing" cellpadding="0" cellspacing="0">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td>{{ entity.id }}</td>
            <td>{{ entity.username }}</td>
            <td>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
<div class="select">
	<strong>Other Pages: </strong>
	<select>
		<option>1</option>
	</select>
</div>
</div>
{% endblock %}
